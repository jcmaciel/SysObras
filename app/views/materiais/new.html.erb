<h1>New material</h1>

<% form_for(@material) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :nome %><br />
    <%= f.text_field :nome %>
  </p>
  <p>
    <%= f.label :quantidade %><br />
    <%= f.text_field :quantidade %>
  </p>
  <p>
    <%= f.label :valor %><br />
    <%= f.text_field :valor %>
  </p>
  <p>
    <%= f.label :data_da_compra %><br />
    <%= f.date_select :data_da_compra %>
  </p>
   <p>
    <%= f.label :fornecedor_id %><br />
    <%= f.text_field :fornecedor_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', materiais_path %>

